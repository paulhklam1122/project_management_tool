<div class="container">
  <h1>Edit Project</h1>
  <%= @project.errors.full_messages.join(", ") %>
  <%= simple_form_for @project do |f| %>
    <div class="text-left" >
      <%= f.input :title %>
      <%= f.input :description %>
      <%= f.input :due_date %>
    </div>
      <%= f.button :submit, class: "btn btn-default btn-primary" %>
  <% end %>
</div>
