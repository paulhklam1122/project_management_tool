<div class="container">
  <h1>Edit Discussion</h1>
  <%= @discussion.errors.full_messages.join(", ") %>
  <%= simple_form_for [@project, @discussion] do |f| %>
    <div class="text-left" >
      <%= f.input :title %>
      <%= f.input :body %>
    </div>
      <%= f.button :submit, class: "btn btn-default btn-primary"%>
  <% end %>
</div>
